# This is Git's per-user configuration file.
[user]
	email = Kevin@Kevinbrink.com
	name = Kevin Brink
[core]
	editor = /usr/bin/vim
[diff]
	tool = vimdiff
[alias]
  logg = git log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
[credential]
	helper = cache --timeout=172800
